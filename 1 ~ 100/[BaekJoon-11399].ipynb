   "source": [
    "n = int(input())\n",
    "p = list(map(int,input().split()))\n",
    "\n",
    "p.sort()\n",
    "\n",
    "sum = 0\n",
    "\n",
    "for i in range(0,n) :\n",
    "  sum += p[i]\n",
    "  for k in range(0,i) :\n",
    "   sum += p[k]\n",
    "    \n",
    "print(sum)"
   ]
