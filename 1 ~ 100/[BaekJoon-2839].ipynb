   "source": [
    "n = int(input()) #kg \n",
    "\n",
    "count = 0\n",
    " \n",
    "while n >= 0 :\n",
    "  if n % 5 == 0 : \n",
    "    count += n / 5\n",
    "    n = 0 \n",
    "    break\n",
    "  else :\n",
    "    n -= 3\n",
    "    count += 1\n",
    "    \n",
    "  \n",
    "if n == 0 :\n",
    "  print(int(count))\n",
    "else : \n",
    "  print('-1')"
   ]
  }
